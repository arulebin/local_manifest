<?xml version="1.0" encoding="UTF-8"?>
<manifest>

        <remote  name="Ebin"
          fetch="https://github.com/arulebin" />
        <remote  name="pulkit"
          fetch="https://github.com/Pulkit077" />
        <remote name="gcc"
          fetch="https://github.com/mvaisakh" />

        <!-- Device tree -->
        <project path="device/xiaomi/rosy" name="device_xiaomi_rosy" remote="Ebin" revision="styx" clone-depth="1" />
